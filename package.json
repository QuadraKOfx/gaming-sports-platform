{
  "name": "gaming-sports-platform",
  "version": "0.0.0",
  "description": "",
  "main": "main.js",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "start": "electron .",
    "watch": "webpack --config webpack.common.js --watch",
    "build:prod": "webpack --config webpack.prod.js",
    "publish": "electron-builder --win -p always",
    "pack": "electron-builder --dir",
    "dist": "electron-builder --win",
    "postinstall": "electron-builder install-app-deps"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/QuadraKOfx/gaming-sports-platform.git"
  },
  "author": "",
  "license": "ISC",
  "build": {
    "publish": [
      {
        "provider": "github",
        "owner": "QuadraKOfx",
        "repo": "gaming-sports-platform"
      }
    ],
    "productName": "Legendary Arena",
    "appId": "com.quadrako.gamearena",
    "files": [
      "build/**/*",
      "assets/**/*",
      "utils/**/*",
      "index.html",
      "splash.html",
      "splash.css",
      "main.js",
      "preload.js"
    ],
    "win": {
      "icon": "assets/images/react_app_logo.png",
      "target": [
        "nsis",
        "zip"
      ]
    },
    "linux": {
      "target": [
        "deb",
        "rpm",
        "AppImage"
      ],
      "category": "Development"
    }
  },
  "bugs": {
    "url": "https://github.com/QuadraKOfx/gaming-sports-platform/issues"
  },
  "devDependencies": {
    "dotenv-webpack": "^7.0.3",
    "electron": "^13.2.1",
    "electron-builder": "^22.11.7",
    "electron-reload": "^1.5.0"
  },
  "dependencies": {
    "@babel/core": "^7.15.0",
    "@babel/preset-env": "^7.15.0",
    "@babel/preset-react": "^7.14.5",
    "@reduxjs/toolkit": "^1.4.0",
    "babel-loader": "^8.2.2",
    "bootstrap": "^5.1.0",
    "bootstrap-icons": "^1.5.0",
    "css-loader": "^4.3.0",
    "electron-is-dev": "^2.0.0",
    "electron-squirrel-startup": "^1.0.0",
    "feather-icons": "^4.28.0",
    "firebase": "^7.24.0",
    "moment": "^2.29.1",
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "react-feather": "^2.0.9",
    "react-hook-form": "^6.8.1",
    "react-redux": "^7.2.1",
    "react-router-dom": "^5.2.0",
    "redux": "^4.0.5",
    "redux-thunk": "^2.3.0",
    "sass": "^1.38.0",
    "sass-loader": "^10.2.0",
    "style-loader": "^3.2.1",
    "webpack": "^5.50.0",
    "webpack-cli": "^4.8.0",
    "webpack-merge": "^5.8.0"
  }
}
